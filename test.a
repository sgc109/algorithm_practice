.data

A:	.word	0
B:	.word	0

.text
.globl main

readInt:
	li		$v0,	5
	syscall
	sw		$v0,	0($a0)
	jr		$ra

fact:	
	li		$t0,	0
	li		$t1,	1
	beq		$t0,	$a0,	return
	beq		$t1,	$a0,	return
	
	addi	$sp,	$sp,		-12
	sw		$fp,	4($sp)
	sw		$ra,	8($sp)
	la		$fp,	12($sp)

	sw		$a0,	0($sp)
	addi	$a0,	$a0,	-1
	jal		fact
	lw		$t2,	0($sp)
	mult	$v0,	$t2
	mflo	$v0

	lw		$ra,	8($sp)
	lw		$fp,	4($sp)
	addi	$sp,	$sp,		12

	jr		$ra

return:
	li		$v0,	1
	jr		$ra
	
main:
	la		$a0,	A
	jal		readInt

	lw		$t6,	A
	add		$a0,	$0,		$t6
	jal		fact

	add		$a0,	$0,		$v0		# print fact(A)
	li		$v0,	1
	syscall

	li		$v0,	10
	syscall

